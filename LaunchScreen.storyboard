<?xml version="1.0" encoding="UTF-8"?>
<document type="com.apple.InterfaceBuilder3.CocoaTouch.Storyboard.XIB" version="3.0"
          toolsVersion="21507" targetRuntime="iOS.CocoaTouch" propertyAccessControl="none"
          useAutolayout="YES" launchScreen="YES" colorMatched="YES" initialViewController="VC">
  <device id="retina6_12" orientation="portrait" appearance="light"/>
  <dependencies>
    <plugIn identifier="com.apple.InterfaceBuilder.IBCocoaTouchPlugin" version="21505"/>
    <capability name="documents saved in the Xcode 8 format" minToolsVersion="8.0"/>
  </dependencies>
  <scenes>
    <scene sceneID="scene">
      <objects>
        <viewController storyboardIdentifier="LaunchScreen" id="VC" sceneMemberID="viewController">
          <view key="view" contentMode="scaleToFill" id="root">
            <rect key="frame" x="0" y="0" width="393" height="852"/>
            <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
            <subviews>
              <!-- Background image fills the screen -->
              <imageView userInteractionEnabled="NO" contentMode="scaleAspectFill"
                         image="launch-screen-background" translatesAutoresizingMaskIntoConstraints="NO" id="bg"/>
              <!-- Centered logo -->
              <imageView userInteractionEnabled="NO" contentMode="scaleAspectFit"
                         image="logo" translatesAutoresizingMaskIntoConstraints="NO" id="lg"/>
            </subviews>

            <constraints>
              <!-- Pin background to edges -->
              <constraint firstItem="bg" firstAttribute="top" secondItem="root" secondAttribute="top" id="c1"/>
              <constraint firstItem="bg" firstAttribute="leading" secondItem="root" secondAttribute="leading" id="c2"/>
              <constraint firstItem="root" firstAttribute="trailing" secondItem="bg" secondAttribute="trailing" id="c3"/>
              <constraint firstItem="root" firstAttribute="bottom" secondItem="bg" secondAttribute="bottom" id="c4"/>

              <!-- Center logo -->
              <constraint firstItem="lg" firstAttribute="centerX" secondItem="root" secondAttribute="centerX" id="c5"/>
              <constraint firstItem="lg" firstAttribute="centerY" secondItem="root" secondAttribute="centerY" id="c6"/>

              <!-- Size logo: 40% of screen width; height = width -->
              <constraint firstItem="lg" firstAttribute="width" secondItem="root" secondAttribute="width" multiplier="0.40" id="c7"/>
              <constraint firstItem="lg" firstAttribute="height" secondItem="lg" secondAttribute="width" multiplier="1" id="c8"/>
            </constraints>

            <viewLayoutGuide key="safeArea" id="safe"/>
          </view>
        </viewController>
        <placeholder placeholderIdentifier="IBFirstResponder" id="FR" userLabel="First Responder" sceneMemberID="firstResponder"/>
      </objects>
    </scene>
  </scenes>

  <!-- Not strictly required, but helps IB preview; sizes will be ignored at runtime -->
  <resources>
    <image name="launch-screen-background" width="1024" height="1024"/>
    <image name="logo" width="1024" height="1024"/>
  </resources>
</document>
